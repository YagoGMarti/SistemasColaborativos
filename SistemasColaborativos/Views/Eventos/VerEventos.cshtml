@model IEnumerable<SistemasColaborativos.Models.Turno>

@if (Model != null)
{
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Notas)
            </th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Notas)
                </td>
            </tr>
        }
    </table>
}

